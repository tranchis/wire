                                monitoTemplateParameter = of.createTemplateParameter();
                                monitoTemplateParameter.setName("statename");
                                monitoTemplateParameter.setDescription("statename");
                                monitoTemplateParameter.setRestrictions("statename in ('SPAIN','ITALY','FINLAND')");
                                monitoTemplateParameter.setId("statename");
                                monitoTemplateParameter.setMandatory(true);
                                monitoTemplateParameter.setMaxelements(1);
